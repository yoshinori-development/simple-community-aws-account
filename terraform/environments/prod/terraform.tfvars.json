{
  "region": "ap-northeast-1",
  "name": "simple-community",
  "shortname": "sc",
  "env": "prod",
  "administrator_role_arn": "arn:aws:iam::278706633282:role/aws-reserved/sso.amazonaws.com/ap-northeast-1/AWSReservedSSO_AdministratorAccess_c057b0dca7c36fad",
  "domain": "dev.sc.yoshinori-satoh.net",
  "ssm": {
    "allow_session_manager_role_arns": [
      "arn:aws:iam::278706633282:role/aws-reserved/sso.amazonaws.com/ap-northeast-1/AWSReservedSSO_AdministratorAccess_c057b0dca7c36fad"
    ]
  },
  "network": {
    "vpc": {
      "cidr_block": "10.0.0.0/16"
    },
    "subnets": {
      "public": {
        "a": {
          "cidr_block": "10.0.0.0/20"
        },
        "c": {
          "cidr_block": "10.0.16.0/20"
        }
      },
      "application": {
        "a": {
          "cidr_block": "10.0.32.0/20"
        },
        "c": {
          "cidr_block": "10.0.48.0/20"
        }
      },
      "database": {
        "a": {
          "cidr_block": "10.0.128.0/24"
        },
        "c": {
          "cidr_block": "10.0.129.0/24"
        }
      },
      "tooling": {
        "cidr_block": "10.0.130.0/24"
      }
    },
    "nat_instance": {
      "ami": "ami-0443cbee57a3b2490",
      "instance_type": "t4g.nano"
    },
    "bastion": {
      "ami_name_filter": "amzn2-ami-hvm-*-arm64-gp2",
      "instance_type": "t4g.nano"
    }
  },
  "tooling": {
    "instance": {
      "ami": "ami-0eaa93483df72de28",
      "instance_type": "t4g.nano"
    }
  },
  "rds": {
    "core": {
      "allowed_security_group_ids": [],
      "ssm_parameters": {
        "database_password": {
          "name": "/rds/core/prod/password",
          "with_decryption": true
        }
      },
      "db_instance": {
        "instance_class": "db.t3.medium",
        "engine_version": "8.0.25",
        "identifier": "core",
        "multi_az": "false",
        "port": "3306",
        "dbname": "core",
        "storage_type": "gp2",
        "allocated_storage": "20",
        "max_allocated_storage": "50",
        "allow_major_version_upgrade": false,
        "auto_minor_version_upgrade": true,
        "publicly_accessible": false,
        "username": "application",
        "iam_database_authentication_enabled": true,
        "performance_insights_enabled": true,
        "storage_encrypted": true,
        "delete_automated_backups": true,
        "deletion_protection": false,
        "backup_retention_period": 7,
        "backup_window": "20:00-20:30",
        "maintenance_window": "sun:22:00-sun:22:30",
        "enabled_cloudwatch_logs_exports": ["audit", "error", "slowquery"],
        "monitoring_interval": 60
      },
      "alarm": {
        "thresholds": {
          "cpu_utilization": "80",
          "cpu_credit_balance": "144",
          "free_storage_space": "10737418240",
          "freeable_memory": "104857600",
          "swap_usage": "104857600",
          "connections": "50",
          "burst_balance": "70",
          "ebs_io_balance": "70",
          "ebs_byte_balance": "70",
          "read_iops": "150",
          "write_iops": "150",
          "read_throughtput": "100",
          "write_throughtput": "100",
          "network_receive_throughtput": "2048",
          "network_transmit_throughtput": "2048"
        }
      }
    }
  },
  "ecs_cluster_name": "simple-community-prod",
  "ecr_repositories": [
    "simple-community-api-core",
    "simple-community-app-community"
  ],
  "ecs_services": {
    "api_core": {
      "env": "",
      "ecs_task_definition": {
        "name": "simple-community-prod-api-core"
      },
      "desired_count": 1,
      "container": {
        "name": "api",
        "port": 8080
      },
      "alarm_thresholds": {
        "cpu_utilization": 80,
        "memory_utilization": 80
      }
    },
    "app_community": {
      "env": "",
      "ecs_task_definition": {
        "name": "simple-community-prod-app-community"
      },
      "desired_count": 1,
      "dns": {
        "external_host": ""
      },
      "container": {
        "name": "app",
        "port": 80
      },
      "alarm_thresholds": {
        "cpu_utilization": 80,
        "memory_utilization": 80
      }
    }
  },
  "ssm_parameter_prefix": "/simple-community/prod",
  "service_discovery_namespace": "simple-community-prod"
}
